//class com constructor
class Pessoa{
	nome;
	idade;
	anoDeNascimento;
   //constructor podemos preparar atributos, funções, Etc..  de um objeto, pessoa, Etc.. 
	constructor(nome, idade){
		this.nome = nome;
		this.idade = idade;
		this.anoDeNascimento = 2022 - idade;
	}

	descrever(){
		console.log(`Meu nome è ${this.nome} e minha idade é ${this.idade}`);
	}
}
//Utilizando a função para comparar a idade de pessoas cujo foram instanciada
function compararPessoas(p1, p2){
	if(p1.idade > p2.idade){
        console.log(`${p1.nome} é mais velho(a) que ${p2.nome}`)
	}else if(p2.idade > p1.idade){
		console.log(`${p1.nome} é mais velho(a) que ${p2.nome}`)
	}else{
		console.log(`${p1.nome} e ${p2.nome} tem a mesma idade`)
	}
}

//instancia uma pessoa com seus atributos
const vitor = new Pessoa('Vitor', 25);
const renan = new Pessoa('Renan', 30);
const fernando = new Pessoa('Fenando', 35);

compararPessoas(fernando, vitor);

