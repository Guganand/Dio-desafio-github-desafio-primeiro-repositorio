
/* 
Uma sala contem 5 alunos e para cada aluno foi sorteado um número de 1 -100.
Faça um programa que receba os 5 números sorteados para os alunos e mostre o maior número sorteado.

Dados de entrada:

5, 50, 10, 98, 23;

saida: 98;
*/

//////////////////////////////////////////////////

/*
//(OBS) Pra mim => So pra entender como funciona
const { gets, print } = require('./exercicio_maiorNumero');

let numeroSorteados = [];

for (let i = 0; i < 5; i++) {
    const numeroPorLoop = gets();
    numeroSorteados.push(numeroPorLoop);
}

//print(numeroSorteados)

let maiorNumero = 0;
for (let i = 0; i < numeroSorteados.length; i++) {
    const numeroPorLoop = numeroSorteados[i];
    if (numeroPorLoop > maiorNumero) {
        maiorNumero = numeroPorLoop;

    }

}
print(maiorNumero); 
*/
const { gets, print } = require('./exercicio_maiorNumero');

let maiorNumero = 0;

for (let i = 0; i < 5; i++) {
    const numeroPorLoop = gets();
    if(numeroPorLoop > maiorNumero){
        maiorNumero = numeroPorLoop;
    }

}


print(maiorNumero);
